{
  "name": "dssim-bin",
  "version": "1.3.3",
  "description": "dssim-bin wrapper that makes it seamlessly available as a local dependency",
  "author": "Frederic Hemberger (https://frederic-hemberger.de/)",
  "repository": "https://github.com/fhemberger/dssim-bin.git",
  "license": "MIT",
  "engines": {
    "node": ">= 4.2"
  },
  "bin": {
    "dssim": "cli.js"
  },
  "files": [
    "lib",
    "cli.js",
    "index.js"
  ],
  "scripts": {
    "postinstall": "npm run build",
    "build": "cross-env CARGO_TARGET_DIR=./vendor CARGO_TERM_VERBOSE=true node lib/install.js",
    "test": "mocha --timeout 50000"
  },
  "keywords": [
    "image",
    "img",
    "png",
    "dssim"
  ],
  "dependencies": {
    "bin-build": "^3.0.0",
    "bin-wrapper": "^3.0.0",
    "cross-env": "^5.1.1",
    "logalot": "^2.0.0"
  },
  "devDependencies": {
    "bin-check": "^4.1.0",
    "mkdirp": "^0.5.0",
    "mocha": "^4.0.1",
    "rimraf": "^2.6.2"
  }
}
